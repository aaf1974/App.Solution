{
  "Serilog": {
    "IsStart": true, //флаг запуска логирования Serilog
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:/AppLog.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "AppDbContext": "Host=localhost;Port=5432;Database=App.Data;Username=postgres;Password=postgres"
  },
  "LoggingConfig": {
    "SerilogLogFile": "D:\\AppTemplate.log"
  }

}